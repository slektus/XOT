@echo off
color 0c
title (XOT )
echo off & cls
color 5
echo.
ping localhost >nul
color 02
set /p IP=[+]-Enter IP:
echo XOT %Ip%
echo ===========================================
:top
PING  -n 1 %IP% | FIND "TTL="
IF ERRORLEVEL 1 echo %IP% 
color 01
ping -t 2 0 1 127.0.0.1 >nul
color 02
ping -t 2 0 1 127.0.0.1 >nul
